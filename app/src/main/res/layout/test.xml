<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/up_back1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/up_0"
            app:layout_constraintStart_toStartOf="@+id/up1"
            app:layout_constraintTop_toTopOf="@+id/up1" />

        <ImageView
            android:id="@id/up1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:src="@drawable/up_0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/down_back1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/up_back1"
            android:src="@drawable/down_0"
            app:layout_constraintStart_toStartOf="@+id/down1"
            app:layout_constraintTop_toTopOf="@+id/down1" />

        <ImageView
            android:id="@+id/down1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/up_back1"
            android:src="@drawable/down_0"
            app:layout_constraintStart_toStartOf="@+id/up1"
            app:layout_constraintTop_toBottomOf="@+id/up1" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:onClick="check"
        android:text="change"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayout4"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        app:layout_constraintStart_toEndOf="@+id/constraintLayout"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout">

        <ImageView
            android:id="@+id/up_back2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/up_0"
            app:layout_constraintStart_toStartOf="@+id/up2"
            app:layout_constraintTop_toTopOf="@+id/up2" />

        <ImageView
            android:id="@id/up2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:src="@drawable/up_0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/down_back2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/up_back2"
            android:src="@drawable/down_0"
            app:layout_constraintStart_toStartOf="@+id/down2"
            app:layout_constraintTop_toTopOf="@+id/down2" />

        <ImageView
            android:id="@+id/down2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/up_back2"
            android:src="@drawable/down_0"
            app:layout_constraintStart_toStartOf="@+id/up2"
            app:layout_constraintTop_toBottomOf="@+id/up2" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        app:layout_constraintStart_toEndOf="@+id/constraintLayout1"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout1">

        <ImageView
            android:id="@+id/up_back3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/up_0"
            app:layout_constraintStart_toStartOf="@+id/up3"
            app:layout_constraintTop_toTopOf="@+id/up3" />

        <ImageView
            android:id="@id/up3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:src="@drawable/up_0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/down_back3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/up_back3"
            android:src="@drawable/down_0"
            app:layout_constraintStart_toStartOf="@+id/down3"
            app:layout_constraintTop_toTopOf="@+id/down3" />

        <ImageView
            android:id="@+id/down3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/up_back3"
            android:src="@drawable/down_0"
            app:layout_constraintStart_toStartOf="@+id/up3"
            app:layout_constraintTop_toBottomOf="@+id/up3" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        app:layout_constraintStart_toEndOf="@+id/constraintLayout3"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout3">

        <ImageView
            android:id="@+id/up_back4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/up_0"
            app:layout_constraintStart_toStartOf="@+id/up4"
            app:layout_constraintTop_toTopOf="@+id/up4" />

        <ImageView
            android:id="@id/up4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:src="@drawable/up_0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/down_back4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/up_back4"
            android:src="@drawable/down_0"
            app:layout_constraintStart_toStartOf="@+id/down4"
            app:layout_constraintTop_toTopOf="@+id/down4" />

        <ImageView
            android:id="@+id/down4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/up_back4"
            android:src="@drawable/down_0"
            app:layout_constraintStart_toStartOf="@+id/up4"
            app:layout_constraintTop_toBottomOf="@+id/up4" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>